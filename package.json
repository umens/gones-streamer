{
  "name": "gones-streamer",
  "description": "App for US football - streaming UI",
  "version": "1.3.0",
  "author": "Ulysse Mensa",
  "main": "main.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:electron": "ng build --base-href ./",
    "build:electron:prod": "ng build --prod --base-href ./",
    "test": "ng test",
    "test_headless": "ng test --codeCoverage=true --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e_headless": "ng e2e --protractor-config='e2e/ci.protractor.conf.js'",
    "all_tests": "yarn lint && yarn test_headless && yarn e2e_headless",
    "hmr": "ng serve --configuration hmr",
    "deploy": "npx ngh --repo=https://GH_TOKEN@github.com/umens/gones-streamer.git --dir=dist/gones-streamer --name=\"Deployment Bot\" --email=ulysse.mensa@gmail.com",
    "docs": "npx compodoc -p tsconfig.app.json",
    "docs:ci": "npx compodoc -p tsconfig.doc.json -d dist/gones-streamer/documentation",
    "version:patch": "npm --no-git-tag-version version patch",
    "version:minor": "npm --no-git-tag-version version minor",
    "version:major": "npm --no-git-tag-version version major",
    "changelog": "gren changelog -o -G --tags=all -T $GH_TOKEN",
    "release": "gren release -T $GH_TOKEN",
    "start:electron": "ng build --base-href ./ && electron .",
    "electron": "ng build --base-href ./ && tsc --p electron && electron .",
    "electronapp": "tsc --p electron && electron .",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:dev": "wait-on http-get://localhost:4200/ && yarn run electron:serve-tsc && electron . --serve",
    "electron:local": "yarn run build:electron && electron .",
    "electron:linux": "yarn run build:electron:prod && electron-builder build --linux",
    "electron:windows": "yarn run build:electron:prod && electron-builder build --windows",
    "electron:mac": "yarn run build:electron:prod && electron-builder build --mac",
    "electron:build": "electron-builder build --windows"
  },
  "private": false,
  "dependencies": {
    "@agm/core": "^1.0.0",
    "@angular/animations": "~8.2.13",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.13",
    "@angular/compiler": "~8.2.13",
    "@angular/core": "~8.2.13",
    "@angular/forms": "~8.2.13",
    "@angular/platform-browser": "~8.2.13",
    "@angular/platform-browser-dynamic": "~8.2.13",
    "@angular/router": "~8.2.13",
    "@asymmetrik/ngx-leaflet": "^6.0.1",
    "@nebular/auth": "^4.5.0",
    "@nebular/eva-icons": "^4.5.0",
    "@nebular/security": "^4.5.0",
    "@nebular/theme": "^4.5.0",
    "@sweetalert2/ngx-sweetalert2": "^7.0.0",
    "@swimlane/ngx-charts": "^12.0.1",
    "angular-tree-component": "^8.4.1",
    "angular2-chartjs": "^0.5.1",
    "angular2-toaster": "^8.0.0",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.9.2",
    "ckeditor": "^4.12.1",
    "classlist.js": "^1.1.20150312",
    "echarts": "^4.4.0",
    "eva-icons": "^1.1.2",
    "intl": "^1.2.5",
    "ionicons": "^4.6.3",
    "leaflet": "^1.5.1",
    "nebular-icons": "^1.1.0",
    "ng2-ckeditor": "^1.2.6",
    "ng2-completer": "^3.0.2",
    "ng2-smart-table": "^1.5.0",
    "ngx-color": "^4.1.0",
    "ngx-echarts": "^4.2.2",
    "ngx-electron": "^2.2.0",
    "ngx-take-until-destroy": "^5.4.0",
    "normalize.css": "^8.0.1",
    "pace-js": "^1.0.2",
    "roboto-fontface": "^0.10.0",
    "rxjs": "~6.5.3",
    "sha.js": "^2.4.11",
    "socicon": "^3.0.5",
    "sweetalert2": "^9.1.5",
    "tinymce": "^5.1.1",
    "tslib": "^1.10.0",
    "typeface-exo": "^0.0.61",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2"
