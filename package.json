{
  "name": "gones-streamer",
  "version": "1.3.0-development",
  "private": true,
  "homepage": ".",
  "main": "main.bundle.js",
  "scripts": {
    "start": "concurrently \"cross-env BROWSER=none yarn react:start\" \"wait-on http://localhost:3000 && yarn electron:start\"",
    "build": "yarn react:build && yarn electron:build",
    "react:start": "react-scripts start",
    "react:build": "react-scripts build",
    "react:test": "react-scripts test",
    "react:eject": "react-scripts eject",
    "postinstall": "husky install && electron-builder install-app-deps",
    "electron:start": "webpack --config ./electron/webpack.dev.js",
    "electron:build": "webpack --config ./electron/webpack.prod.js",
    "package": "yarn build && electron-builder",
    "release": "electron-builder --c.publish.releaseType=release",
    "beta": "electron-builder --c.publish.releaseType=prerelease",
    "getBinary": "node ./scripts/getLatestBins.js",
    "cz": "cz",
    "semantic-release": "semantic-release"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "generateUpdatesFilesForAllChannels": true,
    "appId": "com.electron.gones-streamer",
    "productName": "Gones Streamer",
    "extends": null,
    "publish": {
      "provider": "github"
    },
    "asarUnpack": [
      "node_modules/ffmpeg-static/bin/${os}/${arch}/ffmpeg",
      "node_modules/ffmpeg-static/index.js",
      "node_modules/ffmpeg-static/package.json"
    ],
    "files": [
      "package.json",
      {
        "from": "build",
        "to": ""
      }
    ],
    "extraResources": [
      {
        "from": "assets/bin",
        "to": "bin"
      },
      {
        "from": "assets/appDatas",
        "to": "appDatas"
      },
      {
        "from": "assets/players",
        "to": "players"
      },
      {
        "from": "assets/sponsors",
        "to": "sponsors"
      }
    ],
    "directories": {
      "output": "release/"
    },
    "win": {
      "icon": "logos/logo.png"
    }
  },
  "dependencies": {
    "antd": "^4.7.3",
    "classnames": "^2.2.6",
    "ffmpeg-static": "^4.2.7",
    "ffprobe-static": "^3.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "obs-websocket-js": "^4.0.2",
    "react": "^17.0.1",
    "react-color": "^2.18.1",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.2.1",
    "react-filter-search": "^1.0.11",
    "react-router-dom": "^5.2.0",
    "uuid": "^8.3.1",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@octokit/rest": "^18.0.6",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/classnames": "^2.2.10",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/ffmpeg-static": "^3.0.0",
    "@types/ffprobe-static": "^2.0.0",
    "@types/fluent-ffmpeg": "^2.1.16",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.3",
    "@types/react": "^16.9.53",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/uuid": "^8.3.0",
    "axios": "^0.21.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.2",
    "concurrently": "^5.3.0",
    "conventional-changelog-conventionalcommits": "^4.4.0",
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "electron-devtools-installer": "^3.1.1",
    "electron-first-run": "^3.0.0",
    "electron-is-dev": "^1.2.0",
    "electron-is-packaged": "^1.0.2",
    "electron-log": "^4.2.4",
    "electron-reload": "^1.5.0",
    "electron-store": "^6.0.1",
    "electron-updater": "^4.3.5",
    "extract-zip": "^2.0.1",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^5.0.0-beta.0",
    "project-name-generator": "^2.1.9",
    "react-scripts": "4.0.0",
    "semantic-release": "^17.2.2",
    "ts-loader": "^8.0.7",
    "typescript": "~4.0.3",
    "unzip": "^0.1.11",
    "wait-on": "^5.2.0",
    "webpack": "4.44.2",
    "webpack-cli": "^4.1.0",
    "webpack-electron-reload": "^1.0.1",
    "webpack-merge": "^5.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/umens/gones-streamer.git"
  }
}
