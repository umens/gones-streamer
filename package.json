  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "test_headless": "ng test --codeCoverage=true --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e_headless": "ng e2e --protractor-config='e2e/ci.protractor.conf.js'",
    "all_tests": "yarn lint && yarn test_headless && yarn e2e_headless",
    "hmr": "ng serve --configuration hmr",
    "deploy": "npx ngh --repo=https://GH_TOKEN@github.com/umens/gones-streamer.git --dir=dist/gones-streamer --name=\"Deployment Bot\" --email=ulysse.mensa@gmail.com",
    "docs": "npx compodoc -p tsconfig.app.json",
    "docs:ci": "npx compodoc -p tsconfig.doc.json -d dist/gones-streamer/documentation",
    "version:patch": "npm --no-git-tag-version version patch",
    "version:minor": "npm --no-git-tag-version version minor",
    "version:major": "npm --no-git-tag-version version major",
    "changelog": "gren changelog -o -G --tags=all -T $GH_TOKEN",
    "release": "gren release -T $GH_TOKEN",
    "start:electron": "ng build --base-href ./ && electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
