{
  "name": "gones-streamer",
  "version": "2.0.0",
  "description": "App for US football - streaming UI",
  "homepage": "https://github.com/umens/gones-streamer",
  "author": {
    "name": "Ulysse Mensa",
    "email": "ulysse.mensa@gmail.com"
  },
  "keywords": [
    "obs",
    "angular 8",
    "electron",
    "typescript",
    "eslint",
    "spectron",
    "sass",
    "ui",
    "nebular",
    "streaming"
  ],
  "main": "main.js",
  "private": false,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "yarn electron:serve-tsc && ng build",
    "build:dev": "yarn build -- -c dev",
    "build:prod": "yarn build -- -c production",
    "ng:serve": "ng serve",
    "ng:serve:web": "ng serve -c web -o",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && yarn electron:serve-tsc && electron . --serve",
    "electron:local": "yarn build:prod && electron .",
    "electron:linux": "yarn build:prod && electron-builder build --linux",
    "electron:windows": "yarn build:prod && electron-builder build --windows",
    "electron:mac": "yarn build:prod && electron-builder build --mac",
    "test": "ng test",
    "e2e": "yarn build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "8.4.1",
    "@angular-devkit/build-angular": "^0.900.3",
    "@angular-eslint/builder": "0.0.1-alpha.18",
    "@angular/animations": "9.0.2",
    "@angular/cdk": "9.0.1",
    "@angular/cli": "^9.0.3",
    "@angular/common": "^9.0.2",
    "@angular/compiler": "^9.0.2",
    "@angular/compiler-cli": "^9.0.2",
    "@angular/core": "^9.0.2",
    "@angular/forms": "^9.0.2",
    "@angular/language-service": "^9.0.2",
    "@angular/platform-browser": "^9.0.2",
    "@angular/platform-browser-dynamic": "^9.0.2",
    "@angular/router": "^9.0.2",
    "@nebular/auth": "4.6.0",
    "@nebular/eva-icons": "4.6.0",
    "@nebular/security": "4.6.0",
    "@nebular/theme": "4.6.0",
    "@ngx-translate/core": "^12.1.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@sweetalert2/ngx-sweetalert2": "8.0.0",
    "@types/echarts": "4.4.3",
    "@types/jasmine": "3.3.16",
    "@types/jasminewd2": "2.0.8",
    "@types/mocha": "5.2.7",
    "@types/node": "12.11.1",
    "@types/sha.js": "2.4.0",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "bootstrap": "4.4.1",
    "chai": "4.2.0",
    "classlist.js": "1.1.20150312",
    "codelyzer": "5.1.2",
    "conventional-changelog-cli": "2.0.32",
    "core-js": "3.1.4",
    "cross-env": "7.0.0",
    "echarts": "4.6.0",
    "electron": "8.0.1",
    "electron-builder": "22.3.2",
    "electron-reload": "1.5.0",
    "eslint": "6.8.0",
    "eslint-plugin-import": "2.20.1",
    "eva-icons": "1.1.2",
    "ionicons": "^4.6.3",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.2.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-electron": "6.3.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "mocha": "6.2.2",
    "nebular-icons": "1.1.0",
    "ng2-completer": "3.0.3",
    "ng2-smart-table": "1.5.0",
    "ngx-color": "4.1.1",
    "ngx-echarts": "4.2.2",
    "ngx-take-until-destroy": "5.4.0",
    "npm-run-all": "4.1.5",
    "pace-js": "1.0.2",
    "roboto-fontface": "0.10.0",
    "rxjs": "6.5.4",
    "sha.js": "2.4.11",
    "socicon": "3.0.5",
    "spectron": "10.0.1",
    "sweetalert2": "9.7.2",
    "tinymce": "5.2.0",
    "ts-node": "8.3.0",
    "typeface-exo": "0.0.61",
    "typescript": "3.7.5",
    "wait-on": "4.0.0",
    "web-animations-js": "2.3.2",
    "webdriver-manager": "12.1.7",
    "zone.js": "0.10.2"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "dependencies": {
    "electron-is-packaged": "1.0.2",
    "electron-log": "4.0.6",
    "electron-root-path": "1.0.16",
    "tslib": "1.10.0"
  }
}
