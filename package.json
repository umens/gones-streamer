{
    "name": "gones-streamer",
    "nativescript": {
        "id": "org.nativescript.gonesstreamer"
    },
    "version": "1.2.0",
    "auto-changelog": {
        "template": "keepachangelog",
        "unreleased": true
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build:prod": "ng build --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "hmr": "ng serve --configuration hmr",
        "android": "tns run android --bundle",
        "ios": "tns run ios --bundle",
        "deploy": "npx ngh --repo=https://GH_TOKEN@github.com/umens/gones-streamer.git --dir=dist/gones-streamer --name=\"Deployment Bot\" --email=ulysse.mensa@gmail.com",
        "docs": "npx compodoc -p src/tsconfig.app.json",
        "docs:ci": "npx compodoc -p src/tsconfig.doc.json -d dist/gones-streamer/documentation",
        "changelog:patch": "npm --no-git-tag-version version patch && auto-changelog -p && git add CHANGELOG.md",
        "changelog:minor": "npm --no-git-tag-version version minor && auto-changelog -p && git add CHANGELOG.md",
        "changelog:major": "npm --no-git-tag-version version major && auto-changelog -p && git add CHANGELOG.md"
    },
    "private": false,
    "dependencies": {
        "@angular/animations": "~7.1.0",
        "@angular/common": "~7.1.0",
        "@angular/compiler": "~7.1.0",
        "@angular/core": "~7.1.0",
        "@angular/forms": "~7.1.0",
        "@angular/platform-browser": "~7.1.0",
        "@angular/platform-browser-dynamic": "~7.1.0",
        "@angular/router": "~7.1.0",
        "@sweetalert2/ngx-sweetalert2": "^6.0.1",
        "@types/sha.js": "^2.4.0",
        "bootstrap": "^4.2.1",
        "core-js": "^2.5.4",
        "jquery": "^3.3.1",
        "nativescript-angular": "~7.1.0",
        "nativescript-theme-core": "~1.0.4",
        "ngx-socket-io": "^2.1.1",
        "nyc": "^14.1.1",
        "popper.js": "^1.14.7",
        "reflect-metadata": "~0.1.8",
        "rxjs": "~6.3.3",
        "sha.js": "^2.4.11",
        "sweetalert2": "7.15.1",
        "tns-core-modules": "~5.1.1",
        "zone.js": "^0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.803.9",
        "@angular/cli": "^8.3.9",
        "@angular/compiler-cli": "~8.2.10",
        "@angularclass/hmr": "^2.1.3",
        "@compodoc/compodoc": "^1.1.10",
        "@fortawesome/fontawesome-free": "^5.11.2",
        "@nativescript/schematics": "~0.7.2",
        "@types/jasmine": "3.4.4",
        "@types/jasminewd2": "~2.0.8",
        "@types/node": "~12.7.1212",
        "angular-cli-ghpages": "^0.6.0",
        "auto-changelog": "^1.16.1",
        "codelyzer": "~5.1.2",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~2.1.0",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "nativescript-dev-sass": "~1.7.0",
        "nativescript-dev-typescript": "~0.10.0",
        "nativescript-dev-webpack": "^1.2.1",
        "protractor": "~5.4.2",
        "ts-node": "~8.4.1",
        "tslint": "~5.20.0",
        "typescript": "~3.6.4"
    }
}
