{
  "name": "gones-streamer",
  "version": "2.0.0",
  "private": true,
  "homepage": ".",
  "main": "build/main.bundle.js",
  "scripts": {
    "start": "concurrently \"cross-env BROWSER=none yarn react:start\" \"wait-on http://localhost:3000 && yarn electron:start\"",
    "build": "yarn react:build && yarn electron:build",
    "react:start": "react-scripts start",
    "react:build": "react-scripts build",
    "react:test": "react-scripts test",
    "react:eject": "react-scripts eject",
    "postinstall": "electron-builder install-app-deps && (is-ci || husky install)",
    "electron:start": "webpack --config ./electron/webpack.dev.js",
    "electron:build": "webpack --config ./electron/webpack.prod.js",
    "package": "yarn build && electron-builder",
    "release": "electron-builder -c.extraMetadata.main=main.bundle.js -c.publish.releaseType=release",
    "beta": "electron-builder -c.extraMetadata.main=main.bundle.js -c.publish.releaseType=prerelease",
    "packdev": "yarn build && electron-builder -w -c.extraMetadata.main=main.bundle.js",
    "getBinary": "node ./scripts/getLatestBins.js",
    "setDefaultImages": "node ./scripts/placeholderImage.js",
    "commit": "cross-env HUSKY=0 git-cz",
    "semantic-release": "semantic-release",
    "pretypecheck-renderer": "dts-cb -i electron/preload.ts -o exposedInMainWorld.d.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "generateUpdatesFilesForAllChannels": true,
    "appId": "com.electron.gones-streamer",
    "productName": "Gones Streamer",
    "extends": null,
    "publish": {
      "provider": "github"
    },
    "asarUnpack": [
      "node_modules/ffmpeg-static/bin/${os}/${arch}/ffmpeg",
      "node_modules/ffmpeg-static/index.js",
      "node_modules/ffmpeg-static/package.json"
    ],
    "files": [
      "package.json",
      {
        "from": "build",
        "to": ""
      }
    ],
    "extraResources": [
      {
        "from": "assets/bin",
        "to": "bin"
      },
      {
        "from": "assets/appDatas",
        "to": "appDatas"
      }
    ],
    "directories": {
      "output": "release/"
    },
    "win": {
      "icon": "logos/logo.png",
      "target": [
        "nsis",
        "nsis-web",
        "portable"
      ]
    }
  },
  "dependencies": {
    "@ant-design/charts": "1.3.5",
    "@ant-design/icons": "^4.7.0",
    "antd": "4.8.0",
    "classnames": "^2.3.1",
    "ffmpeg-static": "^4.4.1",
    "ffprobe-static": "^3.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "lodash": "^4.17.21",
    "obs-studio-node": "https://s3-us-west-2.amazonaws.com/obsstudionodes3.streamlabs.com/osn-0.19.27-release-win64.tar.gz",
    "pretty-bytes": "^5.6.0",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.5.3",
    "react-filter-search": "^1.1.11",
    "react-router-dom": "^6.2.1",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@electron/remote": "^2.0.4",
    "@octokit/auth-action": "^1.3.3",
    "@octokit/rest": "^18.12.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/electron-devtools-installer": "^2.2.1",
    "@types/ffmpeg-static": "^3.0.1",
    "@types/ffprobe-static": "^2.0.1",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.13",
    "@types/react": "^17.0.38",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^8.3.4",
    "axios": "^0.25.0",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.2.4",
    "concurrently": "^7.0.0",
    "conventional-changelog-conventionalcommits": "^4.6.3",
    "copy-webpack-plugin": "^6.4.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dts-for-context-bridge": "^0.7.1",
    "electron": "^16.0.8",
    "electron-builder": "^22.14.5",
    "electron-devtools-installer": "^3.2.0",
    "electron-first-run": "^3.0.0",
    "electron-is-dev": "^2.0.0",
    "electron-is-packaged": "^1.0.2",
    "electron-log": "^4.4.5",
    "electron-reload": "^1.5.0",
    "electron-store": "^8.0.1",
    "electron-updater": "4.6.1",
    "extract-zip": "^2.0.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "project-name-generator": "^2.1.9",
    "react-scripts": "^4.0.3",
    "semantic-release": "^19.0.2",
    "ts-loader": "^8.3.0",
    "typescript": "~4.5.5",
    "unzip": "^0.1.11",
    "wait-on": "^6.0.0",
    "webpack": "4.44.2",
    "webpack-cli": "^4.9.2",
    "webpack-electron-reload": "^1.0.1",
    "webpack-merge": "^5.8.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/umens/gones-streamer.git"
  }
}
