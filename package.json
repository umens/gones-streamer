{
  "name": "gones-streamer",
  "version": "2.1.0-beta.6",
  "private": true,
  "homepage": ".",
  "main": "build/main.bundle.js",
  "author": {
    "email": "ulysse.mensa@free.fr",
    "name": "MENSA Ulysse"
  },
  "description": "App that provide easy UI for broadcasting game for your sport team",
  "scripts": {
    "start": "concurrently \"yarn react:start\" \"wait-on http://localhost:3000 && yarn electron:start\"",
    "build": "yarn react:build && yarn electron:build",
    "react:start": "cross-env BROWSER=none react-scripts start",
    "react:build": "react-scripts build",
    "react:test": "react-scripts test",
    "react:eject": "react-scripts eject",
    "postinstall": "electron-builder install-app-deps && (is-ci || husky install)",
    "electron:start": "webpack --config ./electron/webpack.dev.js",
    "electron:build": "webpack --config ./electron/webpack.prod.js",
    "package": "yarn build && electron-builder",
    "release": "electron-builder -c.extraMetadata.main=main.bundle.js -c.publish.releaseType=release",
    "beta": "electron-builder -c.extraMetadata.main=main.bundle.js -c.publish.releaseType=prerelease",
    "packdev": "yarn build && electron-builder -w -c.extraMetadata.main=main.bundle.js",
    "getBinary": "node ./scripts/getLatestBins.js",
    "setDefaultImages": "node ./scripts/placeholderImage.js",
    "commit": "cross-env HUSKY=0 git-cz",
    "semantic-release": "semantic-release",
    "pretypecheck-renderer": "dts-cb -i electron/preload.ts -o exposedInMainWorld.d.ts",
    "prepare": "install-peers",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "generateUpdatesFilesForAllChannels": true,
    "appId": "com.umens.gones-streamer",
    "productName": "Gones Streamer",
    "extends": null,
    "publish": {
      "provider": "github"
    },
    "asarUnpack": [
      "node_modules/ffmpeg-static/bin/${os}/${arch}/ffmpeg",
      "node_modules/ffmpeg-static/index.js",
      "node_modules/ffmpeg-static/package.json"
    ],
    "files": [
      "package.json",
      {
        "from": "build",
        "to": ""
      }
    ],
    "extraResources": [
      {
        "from": "assets/bin",
        "to": "bin"
      },
      {
        "from": "assets/appDatas",
        "to": "appDatas"
      }
    ],
    "directories": {
      "output": "release/"
    },
    "win": {
      "icon": "logos/logo.png",
      "target": [
        "nsis",
        "nsis-web",
        "portable"
      ]
    }
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@nivo/core": "^0.79.0",
    "@nivo/line": "^0.79.1",
    "@nivo/tooltip": "^0.79.0",
    "antd": "^4.20.7",
    "classnames": "^2.3.1",
    "ffmpeg-static": "^5.0.0",
    "ffprobe-static": "^3.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "font-list": "^1.4.5",
    "obs-websocket-js": "^5.0.0-beta.1",
    "pretty-bytes": "^6.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.1.0",
    "react-dropzone": "^14.2.1",
    "react-filter-search": "^1.1.11",
    "react-router-dom": "^6.3.0",
    "react-webcam": "^7.0.1",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/plugin-syntax-flow": "^7.17.12",
    "@babel/plugin-transform-react-jsx": "^7.17.12",
    "@electron/remote": "^2.0.8",
    "@octokit/auth-action": "^1.3.3",
    "@octokit/rest": "^18.12.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/classnames": "^2.3.1",
    "@types/d3-time": "^3.0.0",
    "@types/d3-time-format": "^4.0.0",
    "@types/electron-devtools-installer": "^2.2.2",
    "@types/ffmpeg-static": "^3.0.1",
    "@types/ffprobe-static": "^2.0.1",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/jest": "^27.5.1",
    "@types/node": "^16.11.36",
    "@types/react": "^18.0.9",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^18.0.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^8.3.4",
    "autoprefixer": "^10.4.7",
    "axios": "^0.27.2",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.2.4",
    "concurrently": "^7.2.1",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dts-for-context-bridge": "^0.7.1",
    "electron": "^19.0.1",
    "electron-builder": "^23.0.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-first-run": "^3.0.0",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.7",
    "electron-store": "^8.0.1",
    "electron-updater": "^5.0.1",
    "extract-zip": "^2.0.1",
    "get-file-object-from-local-path": "^1.0.2",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "install-peers-cli": "^2.2.0",
    "is-ci": "^3.0.1",
    "postcss": "^8.4.14",
    "project-name-generator": "^2.1.9",
    "react-scripts": "5.0.1",
    "semantic-release": "^19.0.2",
    "source-map-explorer": "^2.5.2",
    "ts-loader": "^9.3.0",
    "typescript": "~4.7.2",
    "unzip": "^0.1.11",
    "wait-on": "^6.0.1",
    "webpack": "5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-electron-reload": "^1.0.1",
    "webpack-merge": "^5.8.0",
    "webpack-shell-plugin-next": "^2.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/umens/gones-streamer.git"
  }
}
