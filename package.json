{
  "name": "gones-streamer",
  "version": "1.3.0",
  "private": true,
  "homepage": ".",
  "main": "build/main.bundle.js",
  "dependencies": {
    "antd": "^4.6.1",
    "classnames": "^2.2.6",
    "obs-websocket-js": "^4.0.1",
    "react": "^16.13.1",
    "react-color": "^2.18.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.3",
    "react-router-dom": "^5.2.0"
  },
  "scripts": {
    "start": "concurrently \"cross-env BROWSER=none yarn react:start\" \"wait-on http://localhost:3000 && yarn electron:start\"",
    "build": "yarn react:build && yarn electron:build",
    "react:start": "react-scripts start",
    "react:build": "react-scripts build",
    "react:test": "react-scripts test",
    "react:eject": "react-scripts eject",
    "postinstall": "electron-builder install-app-deps",
    "electron:start": "webpack --config ./electron/webpack.dev.js",
    "electron:build": "webpack --config ./electron/webpack.prod.js",
    "package": "yarn build && electron-builder"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "some.id.ofyours",
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "extraResources": [
      {
        "from": "assets",
        "to": "resources",
        "filter": [
          "**/*"
        ]
      }
    ],
    "directories": {
      "output": "release/"
    },
    "win": {
      "category": "your.app.category.type",
      "iconUrl": "path-to-icon.png"
    },
    "mac": {
      "category": "your.app.category.type",
      "iconUrl": "path-to-icon.png"
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "@testing-library/user-event": "^12.1.3",
    "@types/classnames": "^2.2.10",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.48",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^6.0.4",
    "cross-env": "^7.0.2",
    "electron": "^10.1.0",
    "electron-builder": "^22.8.0",
    "electron-devtools-installer": "^3.1.1",
    "electron-first-run": "^3.0.0",
    "electron-is-dev": "^1.2.0",
    "electron-is-packaged": "^1.0.2",
    "electron-log": "^4.2.4",
    "electron-reload": "^1.5.0",
    "electron-root-path": "^1.0.16",
    "electron-store": "^6.0.0",
    "electron-updater": "^4.3.4",
    "html-webpack-plugin": "^4.4.1",
    "react-scripts": "3.4.3",
    "ts-loader": "^8.0.3",
    "typescript": "~4.0.2",
    "wait-on": "^5.2.0",
    "webpack": "4.42.0",
    "webpack-cli": "^3.3.12",
    "webpack-electron-reload": "^1.0.1",
    "webpack-merge": "^5.1.3"
  }
}
